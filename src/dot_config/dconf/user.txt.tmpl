#== Theme ==#

[org/gnome/desktop/wm/preferences]
button-layout='appmenu:close'
num-workspaces=9
titlebar-font='Sans Bold 11'

[org/gnome/desktop/interface]
clock-format='12h'
color-scheme='prefer-dark'
font-name='Sans 11'
document-font-name='Sans 11'
monospace-font-name='Monospace 10'
cursor-theme='Yaru'
gtk-theme='Yaru-dark'
icon-theme='Yaru-dark'
gtk-key-theme='Default'

[org/gnome/desktop/sound]
theme-name='freedesktop'

[org/gnome/shell/extensions/user-theme]
name='Yaru-dark'

[org/gnome/terminal/legacy]
theme-variant='dark'

#== Proxy ==#

[system/proxy]
mode='none'

[system/proxy/http]
host='127.0.0.1'
port={{ .proxy.http }}

[system/proxy/https]
host='127.0.0.1'
port={{ .proxy.http }}

[system/proxy/socks]
host='127.0.0.1'
port={{ .proxy.socks }}

#== Input ==#

[org/gnome/desktop/input-sources]
show-all-sources=true
sources=[('xkb', 'us'), ('ibus', 'rime')]
xkb-options=['ctrl:nocaps', 'caps:ctrl_modifier']

[org/gnome/desktop/peripherals/keyboard]
numlock-state=true

[org/gnome/desktop/peripherals/mouse]
natural-scroll=true

[org/gnome/desktop/peripherals/touchpad]
tap-to-click=true
two-finger-scrolling-enabled=true

#== Extensions ==#

[org/gnome/shell/extensions/appindicator]
legacy-tray-enabled=true
tray-pos='right'

[org/gnome/shell/extensions/clipboard-indicator]
display-mode=2
strip-text=true
next-entry=['<Super>bracketright']
prev-entry=['<Super>bracketleft']

[org/gnome/shell/extensions/dash-to-panel]
isolate-monitors=true
isolate-workspaces=true
animate-appicon-hover=true
appicon-margin=4
appicon-padding=4
dot-position='BOTTOM'
group-apps=true
hot-keys=false
multi-monitors=false
panel-lengths='{"0":100,"1":100}'
panel-positions='{"0":"TOP","1":"TOP"}'
panel-sizes='{"0":32,"1":32}'
scroll-icon-action='CYCLE_WINDOWS'
scroll-panel-action='SWITCH_WORKSPACE'
show-appmenu=false

[org/gnome/shell/extensions/paperwm]
cycle-height-steps=[0.25, 0.5, 0.75]
cycle-width-steps=[0.25, 0.5, 0.75]
horizontal-margin=10
show-window-position-bar=true
show-workspace-indicator=false
vertical-margin=5
vertical-margin-bottom=5
window-gap=10
winprops=@as []

#== Nautilus ==#

[org/gnome/nautilus/compression]
default-compression-format='zip'

[org/gnome/nautilus/list-view]
default-zoom-level='medium'
use-tree-view=true

[org/gnome/nautilus/preferences]
default-folder-viewer='list-view'
migrated-gtk-settings=true
search-filter-time-type='last_modified'
search-view='list-view'

#== Custom keybindings ==

[org/gnome/desktop/wm/keybindings]
close=@as []
maximize=@as []
minimize=@as []
move-to-monitor-down=@as []
move-to-monitor-left=@as []
move-to-monitor-right=@as []
move-to-monitor-up=@as []
move-to-workspace-last=@as []
move-to-workspace-left=@as []
move-to-workspace-right=@as []
switch-applications-backward=@as []
switch-applications=@as []
switch-group-backward=@as []
switch-group=@as []
switch-to-workspace-last=@as []
switch-to-workspace-left=@as []
switch-to-workspace-right=@as []
toggle-fullscreen=@as []
toggle-maximized=@as []
unmaximize=@as []
{{ range $i := untilStep 1 10 1 }}
move-to-workspace-{{ $i }}=['<Super><Shift>{{ $i }}']
switch-to-workspace-{{ $i }}=['<Super>{{ $i }}']
{{ end }}

[org/gnome/mutter/keybindings]
