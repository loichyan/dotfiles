{
  "inbounds": [
    {
      "tag": "socks-in",
      "port": {{ .proxy.socks }},
      "protocol": "socks",
      "settings": { "udp": true },
      "sniffing": {
        "enabled": true,
        "destOverride": ["fakedns"],
        "metadataOnly": false
      }
    },
    {
      "tag": "http-in",
      "port": {{ .proxy.http }},
      "protocol": "http",
      "sniffing": {
        "enabled": true,
        "destOverride": ["fakedns"],
        "metadataOnly": false
      }
    }
  ]
}
