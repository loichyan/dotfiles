{
  "inbounds": [
    {
      "tag": "socks-in",
      "protocol": "socks",
      "settings": { "udp": true },
      "listen": "0.0.0.0",
      "port": {{ .proxy.socks }}
    },
    {
      "tag": "http-in",
      "protocol": "http",
      "listen": "0.0.0.0",
      "port": {{ .proxy.http }}
    }
  ]
}
